{
  "$schema": "http://json.schemastore.org/template",
  "author": "Clement Jean",
  "classifications": [ "Web", "gRPC" ],
  "identity": "ShareAble.Utility.Templates.GrpcServer",
  "name": "gRPC server",
  "shortName": "grpcserver",
  "sourceName": "$safeProjectName$",
  "placeholderFilename": ".keep",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "**/[Bb]in/**",
            "**/[Oo]bj/**",
            ".template.config/**/*",
            "**/*.filelist",
            "**/*.user",
            "**/*.lock.json",
            "**/.vs/**"
          ],
          "rename": {
            "grpcserver.csproj": "$safeProjectName$.csproj"
          }
        }
      ]
    }
  ]
}